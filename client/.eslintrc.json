{
	"extends": ["react-app", "plugin:jsx-a11y/recommended"],
	"plugins": ["jsx-a11y"],
	"rules": {
		"semi": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"eqeqeq": "off",
		"no-unused-vars": ["warn", { "vars": "all", "args": "all", "ignoreRestSiblings": true }]
	}
}
